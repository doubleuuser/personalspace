<div class="feed">
      <% @my_posts.each do |post| %>
      <%# <ul></ul> %>

          <div class="card-post">
            <h2><%= post.title %>
            <li><p><%= post.description %></p></li>
            <button class="btn btn-ghost"> <%= link_to "Edit", edit_post_path(post) %></button>
            <button class="btn btn-ghost"> <%= link_to "Delete", post_path(post), method: :delete, data: { confirm: "Are you sure?" } %></button>
          </div>

      <% end %>
    </div>
